#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Rayane/Desktop/Mini projet github/My Portfolio/my-portfolio/node_modules/.pnpm/sass-embedded@1.93.1/node_modules/sass-embedded/dist/bin/node_modules:/mnt/c/Users/Rayane/Desktop/Mini projet github/My Portfolio/my-portfolio/node_modules/.pnpm/sass-embedded@1.93.1/node_modules/sass-embedded/dist/node_modules:/mnt/c/Users/Rayane/Desktop/Mini projet github/My Portfolio/my-portfolio/node_modules/.pnpm/sass-embedded@1.93.1/node_modules/sass-embedded/node_modules:/mnt/c/Users/Rayane/Desktop/Mini projet github/My Portfolio/my-portfolio/node_modules/.pnpm/sass-embedded@1.93.1/node_modules:/mnt/c/Users/Rayane/Desktop/Mini projet github/My Portfolio/my-portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Rayane/Desktop/Mini projet github/My Portfolio/my-portfolio/node_modules/.pnpm/sass-embedded@1.93.1/node_modules/sass-embedded/dist/bin/node_modules:/mnt/c/Users/Rayane/Desktop/Mini projet github/My Portfolio/my-portfolio/node_modules/.pnpm/sass-embedded@1.93.1/node_modules/sass-embedded/dist/node_modules:/mnt/c/Users/Rayane/Desktop/Mini projet github/My Portfolio/my-portfolio/node_modules/.pnpm/sass-embedded@1.93.1/node_modules/sass-embedded/node_modules:/mnt/c/Users/Rayane/Desktop/Mini projet github/My Portfolio/my-portfolio/node_modules/.pnpm/sass-embedded@1.93.1/node_modules:/mnt/c/Users/Rayane/Desktop/Mini projet github/My Portfolio/my-portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/sass-embedded@1.93.1/node_modules/sass-embedded/dist/bin/sass.js" "$@"
else
  exec node  "$basedir/../.pnpm/sass-embedded@1.93.1/node_modules/sass-embedded/dist/bin/sass.js" "$@"
fi
